{% if sidebar.nav %}
<ul class="sidebar__nav">
  {% for i in navigation[sidebar.nav] %}
  <li>
    <a href="{{ i.url }}">{{ i.title }}</a>
    {% if i.children %}
    <ul>
    {% for i in i.children %}
      <li>
        <a href="{{ i.url }}">{{ i.title }}</a>
      </li>
    {% endfor %}
    </ul>
    {% endif %}
  </li>
  {% endfor %}
</ul>
{% endif %}
