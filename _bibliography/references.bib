  shorttitle = {Modelos portugueses e arquitetura brasileira},
  author = {Lima, Roberto Pastana Teixeira},
  date = {2001},
  institution = {{Universidade Estadual de Campinas, Instituto de Filosofia e Ciências Humanas}},
  location = {{Campinas}},
